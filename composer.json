{
    "name": "altis/seo",
    "description": "SEO module for Altis",
    "type": "package",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Human Made",
            "email": "hello@humanmade.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "humanmade/hm-redirects": "~0.7.0",
        "yoast/wordpress-seo": "^18.3"
    },
    "autoload": {
        "files": [
            "inc/namespace.php",
            "inc/site_verification/namespace.php"
        ],
        "classmap": [
            "inc/opengraph/"
        ]
    },
    "extra": {
        "altis": {
            "install-overrides": [
                "humanmade/hm-redirects",
                "yoast/wordpress-seo",
                "yoast/wordpress-seo-premium"
            ]
        }
    }
}
